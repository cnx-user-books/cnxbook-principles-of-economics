<row>
        <entry>J </entry>
        <entry> $8</entry>
        <entry>50 </entry>
 </row>

<row>
        <entry> K</entry>
        <entry>$9 </entry>
        <entry>70 </entry>
 </row>

<row>
        <entry>L </entry>
        <entry>$10 </entry>
        <entry>80 </entry>
 </row>

<row>
        <entry> M</entry>
        <entry>$11 </entry>
        <entry>88 </entry>
 </row>

<row>
        <entry> N</entry>
        <entry>$12 </entry>
        <entry>95 </entry>
 </row>

<row>
        <entry>P </entry>
